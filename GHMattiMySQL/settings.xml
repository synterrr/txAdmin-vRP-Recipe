<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <setting key="MySQL:UseConvars">false</setting>
  <setting key="MySQL:Server">{{dbHost}}</setting>
  <setting key="MySQL:Port">3306</setting>
  <setting key="MySQL:Database">{{deploymentID}}</setting>
  <setting key="MySQL:Username">{{dbUsername}}</setting>
  <setting key="MySQL:Password">{{dbPassword}}</setting>
  <setting key="MySQL:Debug">true</setting>
</configuration>
